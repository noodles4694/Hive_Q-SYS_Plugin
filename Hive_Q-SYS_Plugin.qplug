function fn_hive_connect_Status(status)
  fn_log_debug("Setting connected status to " .. tostring(status))
  if status == true then
    Controls.online.Boolean = true

    fn_watch_parameters()
    fn_update_info()
    fn_poll_info()
    fn_update_media_folders()
